APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

MEMCACHED_HOST=127.0.0.1

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=mail.infomaniak.com
MAIL_PORT=587
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=ssl
MAIL_FROM_ADDRESS=
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

VITE_APP_NAME="${APP_NAME}"
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"



# SMS Provider (nimba, twilio, vonage)
SMS_PROVIDER=nimba

# NimbaSMS Configuration (https://developers.nimbasms.com/)
NIMBA_SMS_BASE_URL=https://api.nimbasms.com
NIMBA_SMS_SERVICE_ID=
NIMBA_SMS_SECRET=
NIMBA_SMS_SENDER_NAME=GquiOse

# Twilio Configuration (alternative)
TWILIO_SID=
TWILIO_TOKEN=
TWILIO_FROM=

# Push Notifications - FCM (Android)
FCM_SERVER_KEY= # Deprecated - use credentials file instead
FCM_CREDENTIALS_PATH= # Path to Firebase Admin SDK credentials JSON file (e.g., storage/app/firebase/credentials.json)

# Push Notifications - APNs (iOS)
APNS_KEY_ID= # Key ID from Apple Developer Portal
APNS_TEAM_ID= # Team ID from Apple Developer Portal
APNS_BUNDLE_ID=com.gquiose.app # Your app's bundle identifier
APNS_KEY_PATH= # Path to AuthKey.p8 file (e.g., storage/app/apns/AuthKey.p8)
APNS_ENVIRONMENT=production # 'production' for App Store, 'sandbox' for TestFlight


# Google OAuth (from Google Cloud Console > APIs & Services > Credentials)
# Only client IDs are needed for mobile token verification (no secret required)
GOOGLE_CLIENT_ID=your-project-id-xxxxx.apps.googleusercontent.com
GOOGLE_CLIENT_ID_ANDROID=your-project-id-xxxxx.apps.googleusercontent.com
GOOGLE_CLIENT_ID_IOS=your-project-id-xxxxx.apps.googleusercontent.com

# Facebook OAuth
FACEBOOK_APP_ID=your_app_id
FACEBOOK_APP_SECRET=your_secret

# Apple OAuth
APPLE_BUNDLE_ID=com.yourapp
APPLE_TEAM_ID=your_team_id
APPLE_KEY_ID=your_key_id